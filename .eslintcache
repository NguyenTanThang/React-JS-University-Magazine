ersity Magazine\\client\\src\\config\\base.js",[],"E:\\Test Things Out\\University Magazine\\client\\src\\components\\Contribution\\EditContribution.js",["418","419","420","421"],"import React, { Component } from 'react';\r\nimport {Form, Label, Input, CustomInput, FormGroup} from \"reactstrap\";\r\nimport {\r\n    getAllTerms,\r\n    editContribution,\r\n    uploadImageFirebase,\r\n    uploadDocumentFirebase,\r\n    getFacultyByID\r\n} from \"../../requests\";\r\nimport {message} from \"antd\";\r\nimport {\r\n    getFileExtension,\r\n    acceptDocExt,\r\n    acceptImageExt\r\n} from \"../../utils\"\r\n\r\nclass EditContribution extends Component {\r\n\r\n    state = {\r\n        term: \"\",\r\n        docFile: \"\",\r\n        imageFile: \"\",\r\n        faculty: \"\",\r\n        terms: [],\r\n        title: \"\"\r\n    }\r\n\r\n    async componentDidMount() {\r\n        const {contributionItem} = this.props;\r\n        const termData = await getAllTerms();\r\n        const facData = await getFacultyByID(contributionItem.faculty._id);\r\n        this.setState({\r\n            term: contributionItem.term._id,\r\n            terms: termData.data,\r\n            faculty: facData.data,\r\n            title: contributionItem.title\r\n        })\r\n    }\r\n\r\n    renderTermOptions = () => {\r\n        const {terms} = this.state;\r\n        return terms.map(term => {\r\n            return (\r\n                <option value={term._id} key={term._id}>{term.name}</option>\r\n            )\r\n        })\r\n    }\r\n\r\n    handleFileChange = (e) => {\r\n        const targetName = e.target.name;\r\n        if (e.target.files.length === 0) {\r\n            return;\r\n        }\r\n        const file = e.target.files[0];\r\n        const fileExt = getFileExtension(file.name);\r\n\r\n        if (targetName == \"imageFile\") {\r\n            if (acceptImageExt(fileExt)) {\r\n                return this.setState({\r\n                    [e.target.name]: file\r\n                })\